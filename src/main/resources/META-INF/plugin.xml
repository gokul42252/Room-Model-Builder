<idea-plugin>
    <id>Room Model Builder.Room-Model-Builder</id>
    <name>Room Table Builder</name>
    <vendor email="" url="">Cretlabs</vendor>
    <version>1.0</version>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="173.0"/>
    <description><![CDATA[
    Plugin convert the Json data into room database model.<br>
    ]]></description>



    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action id="actions.RoomSchemaAction" class="actions.RoomSchemaAction" text="Create Room Model from Json"
                description="creates room model classes from the json in the specified directory" icon="/icons/add.png">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>

   
    </actions>
</idea-plugin>
